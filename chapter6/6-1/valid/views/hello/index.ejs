<!-- 表示したいものをviewsに置く -->
<!DOCTYPE html>
<html lang="ja">
    <head>
        <meta http-equiv="content-type"
            conten="text/html; charset=UTF-8">
            <title><%=title %></title>
            <!-- <script src="https://code.jquery.com/jquery.min.js"></script> -->
            <link rel='stylesheet' href='/stylesheets/style.css' />
    </head>
    <body>
        <head>
            <h1><%=title %></h1>
        </head>
        <div role="main">
            <table>
                <% for(var i in content) {%>
                    <tr>
                        <% var obj = content[i]; %>
                        <td><%=obj.id%></td>
                        <td><%=obj.name%></td>
                        <td><%=obj.mail%></td>
                        <td><%=obj.age%></td>
                    </tr>
                <% } %>
            </table>
        </div>
    </body>
</html>

